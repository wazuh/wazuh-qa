{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "Analysis alert schema",
  "required": [
    "timestamp",
    "rule",
    "agent",
    "manager",
    "id",
    "full_log",
    "syscheck",
    "decoder",
    "location"
  ],
  "properties": {
    "timestamp": {
      "$id": "#/properties/timestamp",
      "type": "string",
      "title": "The Timestamp Schema",
      "default": "",
      "examples": [
        "2019-10-17T14:36:20.343+0200"
      ],
      "pattern": "^(.*)$"
    },
    "rule": {
      "$id": "#/properties/rule",
      "type": "object",
      "title": "The Rule Schema",
      "required": [
        "level",
        "description",
        "id",
        "firedtimes",
        "mail",
        "groups"
      ],
      "properties": {
        "level": {
          "$id": "#/properties/rule/properties/level",
          "type": "integer",
          "title": "The Level Schema",
          "default": 0,
          "examples": [
            5
          ]
        },
        "description": {
          "$id": "#/properties/rule/properties/description",
          "type": "string",
          "title": "The Description Schema",
          "default": "",
          "examples": [
            "Ossec started again with Mitre information for testing"
          ],
          "pattern": "^(.*)$"
        },
        "id": {
          "$id": "#/properties/rule/properties/id",
          "type": "string",
          "title": "The Id Schema",
          "default": "",
          "examples": [
            "100002"
          ],
          "pattern": "^(.*)$"
        },
        "firedtimes": {
          "$id": "#/properties/rule/properties/firedtimes",
          "type": "integer",
          "title": "The Firedtimes Schema",
          "default": 0,
          "examples": [
            1
          ]
        },
        "mail": {
          "$id": "#/properties/rule/properties/mail",
          "type": "boolean",
          "title": "The Mail Schema",
          "default": false,
          "examples": [
            false
          ]
        },
        "groups": {
          "$id": "#/properties/rule/properties/groups",
          "type": "array",
          "title": "The Groups Schema",
          "items": {
            "$id": "#/properties/rule/properties/groups/items",
            "type": "string",
            "title": "The Items Schema",
            "default": "",
            "examples": [
              "ossec"
            ],
            "pattern": "^(.*)$"
          }
        },
        "pci_dss": {
          "$id": "#/properties/rule/properties/pci_dss",
          "type": "array",
          "title": "The Pci_dss Schema",
          "items": {
            "$id": "#/properties/rule/properties/pci_dss/items",
            "type": "string",
            "title": "The Items Schema",
            "default": "",
            "examples": [
              "10.6.1"
            ],
            "pattern": "^(.*)$"
          }
        },
        "gpg13": {
          "$id": "#/properties/rule/properties/gpg13",
          "type": "array",
          "title": "The Gpg13 Schema",
          "items": {
            "$id": "#/properties/rule/properties/gpg13/items",
            "type": "string",
            "title": "The Items Schema",
            "default": "",
            "examples": [
              "10.1"
            ],
            "pattern": "^(.*)$"
          }
        },
        "gdpr": {
          "$id": "#/properties/rule/properties/gdpr",
          "type": "array",
          "title": "The Gdpr Schema",
          "items": {
            "$id": "#/properties/rule/properties/gdpr/items",
            "type": "string",
            "title": "The Items Schema",
            "default": "",
            "examples": [
              "IV_35.7.d"
            ],
            "pattern": "^(.*)$"
          }
        },
        "hipaa": {
          "$id": "#/properties/rule/properties/hipaa",
          "type": "array",
          "title": "The Hipaa Schema",
          "items": {
            "$id": "#/properties/rule/properties/hipaa/items",
            "type": "string",
            "title": "The Items Schema",
            "default": "",
            "examples": [
              "164.312.b"
            ],
            "pattern": "^(.*)$"
          }
        },
        "nist_800_53": {
          "$id": "#/properties/rule/properties/nist_800_53",
          "type": "array",
          "title": "The Nist_800_53 Schema",
          "items": {
            "$id": "#/properties/rule/properties/nist_800_53/items",
            "type": "string",
            "title": "The Items Schema",
            "default": "",
            "examples": [
              "AU.6"
            ],
            "pattern": "^(.*)$"
          }
        }
      }
    },
    "agent": {
      "$id": "#/properties/agent",
      "type": "object",
      "title": "The Agent Schema",
      "required": [
        "id",
        "name"
      ],
      "properties": {
        "id": {
          "$id": "#/properties/agent/properties/id",
          "type": "string",
          "title": "The Id Schema",
          "default": "",
          "examples": [
            "000"
          ],
          "pattern": "^(.*)$"
        },
        "name": {
          "$id": "#/properties/agent/properties/name",
          "type": "string",
          "title": "The Name Schema",
          "default": "",
          "examples": [
            "wazuh-agent"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "manager": {
      "$id": "#/properties/manager",
      "type": "object",
      "title": "The Manager Schema",
      "required": [
        "name"
      ],
      "properties": {
        "name": {
          "$id": "#/properties/manager/properties/name",
          "type": "string",
          "title": "The Name Schema",
          "default": "",
          "examples": [
            "wazuh-master"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "id": {
      "$id": "#/properties/id",
      "type": "string",
      "title": "The Id Schema",
      "default": "",
      "examples": [
        "1571315780.7434185"
      ],
      "pattern": "^(.*)$"
    },
    "full_log": {
      "$id": "#/properties/full_log",
      "type": "string",
      "title": "The Full_log Schema",
      "default": "",
      "examples": [
        "File '/home/test/file2' added\nMode: real-time\n"
      ],
      "pattern": "(.*)$"
    },
    "syscheck": {
      "$id": "#/properties/syscheck",
      "type": "object",
      "title": "The Syscheck Schema",
      "required": [
        "path",
        "size_after",
        "perm_after",
        "uid_after",
        "gid_after",
        "md5_after",
        "sha1_after",
        "sha256_after",
        "uname_after",
        "gname_after",
        "mtime_after",
        "inode_after",
        "event"
      ],
      "properties": {
        "path": {
          "$id": "#/properties/data/properties/syscheck/properties/path",
          "type": "string",
          "default": "",
          "examples": [
            "/testdir1/example"
          ],
          "pattern": "^(?:\/[^\/]+)*$"
        },
        "size_before": {
          "$id": "#/properties/data/properties/syscheck/properties/size_before",
          "type": "string",
          "default": "0",
          "examples": [
            "4",
            "12"
          ]
        },
        "size_after": {
          "$id": "#/properties/data/properties/syscheck/properties/size_after",
          "type": "string",
          "default": "0",
          "examples": [
            "5",
            "18"
          ]
        },
        "perm_before": {
          "$id": "#/properties/data/properties/syscheck/properties/perm_before",
          "type": "string",
          "default": "",
          "examples": [
            "r--r--r--"
          ],
          "pattern": "^(.*)$"
        },
        "perm_after": {
          "$id": "#/properties/data/properties/syscheck/properties/perm_after",
          "type": "string",
          "default": "",
          "examples": [
            "rw-r--r--"
          ],
          "pattern": "^(.*)$"
        },
        "uid_before": {
          "$id": "#/properties/data/properties/syscheck/properties/uid_before",
          "type": "string",
          "default": "",
          "examples": [
            "0"
          ],
          "pattern": "^[0-9a-fA-F]+$"
        },
        "uid_after": {
          "$id": "#/properties/data/properties/syscheck/properties/uid_after",
          "type": "string",
          "default": "",
          "examples": [
            "1"
          ],
          "pattern": "^[0-9a-fA-F]+$"
        },
        "gid_before": {
          "$id": "#/properties/data/properties/syscheck/properties/gid_before",
          "type": "string",
          "default": "",
          "examples": [
            "0"
          ],
          "pattern": "^[0-9a-fA-F]+$"
        },
        "gid_after": {
          "$id": "#/properties/data/properties/syscheck/properties/gid_after",
          "type": "string",
          "default": "",
          "examples": [
            "1"
          ],
          "pattern": "^[0-9a-fA-F]+$"
        },
        "md5_before": {
          "$id": "#/properties/data/properties/syscheck/properties/md5_before",
          "type": "string",
          "default": "",
          "examples": [
            "d41d8cd98f00b204e9804e7r8ggf984e"
          ],
          "pattern": "^[a-f0-9]{32}$"
        },
        "md5_after": {
          "$id": "#/properties/data/properties/syscheck/properties/md5_after",
          "type": "string",
          "default": "",
          "examples": [
            "d41d8cd98f00b204e9800998ecf8427e"
          ],
          "pattern": "^[a-f0-9]{32}$"
        },
        "sha1_before": {
          "$id": "#/properties/data/properties/syscheck/properties/sha1_before",
          "type": "string",
          "default": "",
          "examples": [
            "da39a3ee5e6b4b0d322d8w1ds5601890afd80709"
          ],
          "pattern": "^[0-9a-f]{5,40}$"
        },
        "sha1_after": {
          "$id": "#/properties/data/properties/syscheck/properties/sha1_after",
          "type": "string",
          "default": "",
          "examples": [
            "da39a3ee5e6b4b0d3255bfef95601890afd80709"
          ],
          "pattern": "^[0-9a-f]{5,40}$"
        },
        "sha256_before": {
          "$id": "#/properties/data/properties/syscheck/properties/sha256_before",
          "type": "string",
          "default": "",
          "examples": [
            "e3b0c44298fc1c149afbf4c8996fb92427ae44458wd1w54ca495991b7852b855"
          ],
          "pattern": "^[a-f0-9]{64}$"
        },
        "sha256_after": {
          "$id": "#/properties/data/properties/syscheck/properties/sha256_after",
          "type": "string",
          "default": "",
          "examples": [
            "e3b0c44298fc1c149afbf4c8996fb92427ae41e4649b934ca495991b7852b855"
          ],
          "pattern": "^[a-f0-9]{64}$"
        },
        "uname_after": {
          "$id": "#/properties/data/properties/syscheck/properties/uname_after",
          "type": "string",
          "default": "",
          "examples": [
            "root"
          ],
          "pattern": "^(.*)$"
        },
        "gname_after": {
          "$id": "#/properties/data/properties/syscheck/properties/gname_after",
          "type": "string",
          "default": "",
          "examples": [
            "root"
          ],
          "pattern": "^(.*)$"
        },
        "mtime_before": {
          "$id": "#/properties/data/properties/syscheck/properties/mtime_before",
          "type": "string",
          "default": "",
          "examples": [
            "2019-12-03T01:01:32"
          ]
        },
        "mtime_after": {
          "$id": "#/properties/data/properties/syscheck/properties/mtime_after",
          "type": "string",
          "default": "",
          "examples": [
            "2019-12-04T01:01:32"
          ]
        },
        "inode_after": {
          "$id": "#/properties/data/properties/syscheck/properties/inode_after",
          "type": "integer",
          "default": 0,
          "examples": [
            18101020
          ]
        },
        "changed_attributes": {
          "$id": "#/properties/data/properties/syscheck/properties/inode_after",
          "type": "array",
          "default": [""],
          "examples": [
            "size",
            "mtime",
            "md5",
            "sha1",
            "sha256"
          ]
        },
        "event": {
          "$id": "#/properties/data/properties/syscheck/properties/gname_after",
          "type": "string",
          "default": "",
          "examples": [
            "root"
          ],
          "pattern": "^(added|modified|deleted)$"
        }
      }
    },
    "decoder": {
      "$id": "#/properties/decoder",
      "type": "object",
      "title": "The Decoder Schema",
      "required": [
        "name"
      ],
      "properties": {
        "name": {
          "$id": "#/properties/decoder/properties/name",
          "type": "string",
          "title": "The Name Schema",
          "default": "",
          "examples": [
            "ossec"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "location": {
      "$id": "#/properties/location",
      "type": "string",
      "title": "The Location Schema",
      "default": "",
      "examples": [
        "ossec-monitord"
      ],
      "pattern": "^(.*)$"
    }
  }
}
