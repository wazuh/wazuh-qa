{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "timestamp",
    "rule",
    "agent",
    "manager",
    "id",
    "cluster",
    "full_log",
    "syscheck",
    "decoder",
    "location"
  ],
  "properties": {
    "timestamp": {
      "$id": "#/properties/timestamp",
      "type": "string",
      "title": "The Timestamp Schema",
      "default": "",
      "examples": [
        "2020-02-14T12:16:42.013+0000"
      ],
      "pattern": "^(.*)$"
    },
    "rule": {
      "$id": "#/properties/rule",
      "type": "object",
      "title": "The Rule Schema",
      "required": [
        "level",
        "description",
        "id",
        "firedtimes",
        "mail",
        "groups",
        "pci_dss",
        "gpg13",
        "gdpr",
        "hipaa",
        "nist_800_53"
      ],
      "properties": {
        "level": {
          "$id": "#/properties/rule/properties/level",
          "type": "integer",
          "title": "The Level Schema",
          "default": 0,
          "examples": [
            7
          ]
        },
        "description": {
          "$id": "#/properties/rule/properties/description",
          "type": "string",
          "title": "The Description Schema",
          "default": "",
          "examples": [
            "Integrity checksum changed."
          ],
          "pattern": "^(.*)$"
        },
        "id": {
          "$id": "#/properties/rule/properties/id",
          "type": "string",
          "title": "The Id Schema",
          "default": "",
          "examples": [
            "550"
          ],
          "pattern": "^(.*)$"
        },
        "firedtimes": {
          "$id": "#/properties/rule/properties/firedtimes",
          "type": "integer",
          "title": "The Firedtimes Schema",
          "default": 0,
          "examples": [
            2
          ]
        },
        "mail": {
          "$id": "#/properties/rule/properties/mail",
          "type": "boolean",
          "title": "The Mail Schema",
          "default": false,
          "examples": [
            false
          ]
        },
        "groups": {
          "$id": "#/properties/rule/properties/groups",
          "type": "array",
          "title": "The Groups Schema",
          "items": {
            "$id": "#/properties/rule/properties/groups/items",
            "type": "string",
            "title": "The Items Schema",
            "default": "",
            "examples": [
              "ossec",
              "syscheck"
            ],
            "pattern": "^(.*)$"
          }
        },
        "pci_dss": {
          "$id": "#/properties/rule/properties/pci_dss",
          "type": "array",
          "title": "The Pci_dss Schema",
          "items": {
            "$id": "#/properties/rule/properties/pci_dss/items",
            "type": "string",
            "title": "The Items Schema",
            "default": "",
            "examples": [
              "11.5"
            ],
            "pattern": "^(.*)$"
          }
        },
        "gpg13": {
          "$id": "#/properties/rule/properties/gpg13",
          "type": "array",
          "title": "The Gpg13 Schema",
          "items": {
            "$id": "#/properties/rule/properties/gpg13/items",
            "type": "string",
            "title": "The Items Schema",
            "default": "",
            "examples": [
              "4.11"
            ],
            "pattern": "^(.*)$"
          }
        },
        "gdpr": {
          "$id": "#/properties/rule/properties/gdpr",
          "type": "array",
          "title": "The Gdpr Schema",
          "items": {
            "$id": "#/properties/rule/properties/gdpr/items",
            "type": "string",
            "title": "The Items Schema",
            "default": "",
            "examples": [
              "II_5.1.f"
            ],
            "pattern": "^(.*)$"
          }
        },
        "hipaa": {
          "$id": "#/properties/rule/properties/hipaa",
          "type": "array",
          "title": "The Hipaa Schema",
          "items": {
            "$id": "#/properties/rule/properties/hipaa/items",
            "type": "string",
            "title": "The Items Schema",
            "default": "",
            "examples": [
              "164.312.c.1",
              "164.312.c.2"
            ],
            "pattern": "^(.*)$"
          }
        },
        "nist_800_53": {
          "$id": "#/properties/rule/properties/nist_800_53",
          "type": "array",
          "title": "The Nist_800_53 Schema",
          "items": {
            "$id": "#/properties/rule/properties/nist_800_53/items",
            "type": "string",
            "title": "The Items Schema",
            "default": "",
            "examples": [
              "SI.7"
            ],
            "pattern": "^(.*)$"
          }
        }
      }
    },
    "agent": {
      "$id": "#/properties/agent",
      "type": "object",
      "title": "The Agent Schema",
      "required": [
        "id",
        "name"
      ],
      "properties": {
        "id": {
          "$id": "#/properties/agent/properties/id",
          "type": "string",
          "title": "The Id Schema",
          "default": "",
          "examples": [
            "000"
          ],
          "pattern": "^(.*)$"
        },
        "name": {
          "$id": "#/properties/agent/properties/name",
          "type": "string",
          "title": "The Name Schema",
          "default": "",
          "examples": [
            "wazuh-master-dev"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "manager": {
      "$id": "#/properties/manager",
      "type": "object",
      "title": "The Manager Schema",
      "required": [
        "name"
      ],
      "properties": {
        "name": {
          "$id": "#/properties/manager/properties/name",
          "type": "string",
          "title": "The Name Schema",
          "default": "",
          "examples": [
            "wazuh-master-dev"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "id": {
      "$id": "#/properties/id",
      "type": "string",
      "title": "The Id Schema",
      "default": "",
      "examples": [
        "1581682602.1491"
      ],
      "pattern": "^(.*)$"
    },
    "cluster": {
      "$id": "#/properties/cluster",
      "type": "object",
      "title": "The Cluster Schema",
      "required": [
        "name",
        "node"
      ],
      "properties": {
        "name": {
          "$id": "#/properties/cluster/properties/name",
          "type": "string",
          "title": "The Name Schema",
          "default": "",
          "examples": [
            "wazuh"
          ],
          "pattern": "^(.*)$"
        },
        "node": {
          "$id": "#/properties/cluster/properties/node",
          "type": "string",
          "title": "The Node Schema",
          "default": "",
          "examples": [
            "master"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "full_log": {
      "$id": "#/properties/full_log",
      "type": "string",
      "title": "The Full_log Schema",
      "default": "",
      "examples": [
        "File '/testdir0/regular' modified\nMode: real-time\nChanged attributes: size,permission,uid,user_name,gid,group_name,mtime,inode,md5,sha1,sha256\nSize changed from '0' to '8'\nPermissions changed from 'rw-r--r--' to 'rw-rw-rw-'\nOwnership was '0', now it is '1'\nUser name was 'root', now it is 'bin'\nGroup ownership was '0', now it is '1'\nGroup name was 'root', now it is 'bin'\nOld modification time was: '1581438201', now it is '1581439247'\nOld inode was: '1185139', now it is '1184275'\nOld md5sum was: 'd41d8cd98f00b204e9800998ecf8427e'\nNew md5sum is : '35e0c8c0b180c95d4e122e55ed62cc64'\nOld sha1sum was: 'da39a3ee5e6b4b0d3255bfef95601890afd80709'\nNew sha1sum is : '19a532c8bc61c311f583455c80ffe37067bbc9bb'\nOld sha256sum was: 'e3b0c44298fc1c149afbf4c8996fb92427ae41e4649b934ca495991b7852b855'\nNew sha256sum is : 'e8ce5dcaf408935ff76747226d2e8bee4319a2f593c1d7a838115e56183d1f37'\n"
      ],
      "pattern": "^[\\s\\S]*$"
    },
    "syscheck": {
      "$id": "#/properties/syscheck",
      "type": "object",
      "title": "The Syscheck Schema",
      "required": [
        "path",
        "event"
      ],
      "properties": {
        "path": {
          "$id": "#/properties/syscheck/properties/path",
          "type": "string",
          "title": "The Path Schema",
          "default": "",
          "examples": [
            "/testdir0/regular"
          ],
          "pattern": "^(?:\/[^\/]+)+$"
        },
        "size_before": {
          "$id": "#/properties/syscheck/properties/size_before",
          "type": "string",
          "title": "The Size_before Schema",
          "default": "",
          "examples": [
            "0"
          ],
          "pattern": "^(.*)$"
        },
        "size_after": {
          "$id": "#/properties/syscheck/properties/size_after",
          "type": "string",
          "title": "The Size_after Schema",
          "default": "",
          "examples": [
            "8"
          ],
          "pattern": "^(.*)$"
        },
        "perm_before": {
          "$id": "#/properties/syscheck/properties/perm_before",
          "type": "string",
          "title": "The Perm_before Schema",
          "default": "",
          "examples": [
            "rw-r--r--"
          ],
          "pattern": "^(.*)$"
        },
        "perm_after": {
          "$id": "#/properties/syscheck/properties/perm_after",
          "type": "string",
          "title": "The Perm_after Schema",
          "default": "",
          "examples": [
            "rw-rw-rw-"
          ],
          "pattern": "^(.*)$"
        },
        "uid_before": {
          "$id": "#/properties/syscheck/properties/uid_before",
          "type": "string",
          "title": "The Uid_before Schema",
          "default": "",
          "examples": [
            "0"
          ],
          "pattern": "^(.*)$"
        },
        "uid_after": {
          "$id": "#/properties/syscheck/properties/uid_after",
          "type": "string",
          "title": "The Uid_after Schema",
          "default": "",
          "examples": [
            "1"
          ],
          "pattern": "^(.*)$"
        },
        "gid_before": {
          "$id": "#/properties/syscheck/properties/gid_before",
          "type": "string",
          "title": "The Gid_before Schema",
          "default": "",
          "examples": [
            "0"
          ],
          "pattern": "^(.*)$"
        },
        "gid_after": {
          "$id": "#/properties/syscheck/properties/gid_after",
          "type": "string",
          "title": "The Gid_after Schema",
          "default": "",
          "examples": [
            "1"
          ],
          "pattern": "^(.*)$"
        },
        "md5_before": {
          "$id": "#/properties/syscheck/properties/md5_before",
          "type": "string",
          "title": "The Md5_before Schema",
          "default": "",
          "examples": [
            "d41d8cd98f00b204e9800998ecf8427e"
          ],
          "pattern": "^(.*)$"
        },
        "md5_after": {
          "$id": "#/properties/syscheck/properties/md5_after",
          "type": "string",
          "title": "The Md5_after Schema",
          "default": "",
          "examples": [
            "35e0c8c0b180c95d4e122e55ed62cc64"
          ],
          "pattern": "^(.*)$"
        },
        "sha1_before": {
          "$id": "#/properties/syscheck/properties/sha1_before",
          "type": "string",
          "title": "The Sha1_before Schema",
          "default": "",
          "examples": [
            "da39a3ee5e6b4b0d3255bfef95601890afd80709"
          ],
          "pattern": "^(.*)$"
        },
        "sha1_after": {
          "$id": "#/properties/syscheck/properties/sha1_after",
          "type": "string",
          "title": "The Sha1_after Schema",
          "default": "",
          "examples": [
            "19a532c8bc61c311f583455c80ffe37067bbc9bb"
          ],
          "pattern": "^(.*)$"
        },
        "sha256_before": {
          "$id": "#/properties/syscheck/properties/sha256_before",
          "type": "string",
          "title": "The Sha256_before Schema",
          "default": "",
          "examples": [
            "e3b0c44298fc1c149afbf4c8996fb92427ae41e4649b934ca495991b7852b855"
          ],
          "pattern": "^(.*)$"
        },
        "sha256_after": {
          "$id": "#/properties/syscheck/properties/sha256_after",
          "type": "string",
          "title": "The Sha256_after Schema",
          "default": "",
          "examples": [
            "e8ce5dcaf408935ff76747226d2e8bee4319a2f593c1d7a838115e56183d1f37"
          ],
          "pattern": "^(.*)$"
        },
        "uname_before": {
          "$id": "#/properties/syscheck/properties/uname_before",
          "type": "string",
          "title": "The Uname_before Schema",
          "default": "",
          "examples": [
            "root"
          ],
          "pattern": "^(.*)$"
        },
        "uname_after": {
          "$id": "#/properties/syscheck/properties/uname_after",
          "type": "string",
          "title": "The Uname_after Schema",
          "default": "",
          "examples": [
            "bin"
          ],
          "pattern": "^(.*)$"
        },
        "gname_before": {
          "$id": "#/properties/syscheck/properties/gname_before",
          "type": "string",
          "title": "The Gname_before Schema",
          "default": "",
          "examples": [
            "root"
          ],
          "pattern": "^(.*)$"
        },
        "gname_after": {
          "$id": "#/properties/syscheck/properties/gname_after",
          "type": "string",
          "title": "The Gname_after Schema",
          "default": "",
          "examples": [
            "bin"
          ],
          "pattern": "^(.*)$"
        },
        "mtime_before": {
          "$id": "#/properties/syscheck/properties/mtime_before",
          "type": "string",
          "title": "The Mtime_before Schema",
          "default": "",
          "examples": [
            "2020-02-11T16:23:21"
          ],
          "pattern": "^(.*)$"
        },
        "mtime_after": {
          "$id": "#/properties/syscheck/properties/mtime_after",
          "type": "string",
          "title": "The Mtime_after Schema",
          "default": "",
          "examples": [
            "2020-02-11T16:40:47"
          ],
          "pattern": "^(.*)$"
        },
        "inode_before": {
          "$id": "#/properties/syscheck/properties/inode_before",
          "type": "integer",
          "title": "The Inode_before Schema",
          "default": 0,
          "examples": [
            1185139
          ]
        },
        "inode_after": {
          "$id": "#/properties/syscheck/properties/inode_after",
          "type": "integer",
          "title": "The Inode_after Schema",
          "default": 0,
          "examples": [
            1184275
          ]
        },
        "diff": {
          "$id": "#/properties/syscheck/properties/diff",
          "type": "string",
          "title": "The Diff Schema",
          "default": "",
          "examples": [
            "0a1\n> Modified\n\\ No newline at end of file\n"
          ],
          "pattern": "^[\\s\\S]*$"
        },
        "tags": {
          "$id": "#/properties/syscheck/properties/tags",
          "type": "array",
          "title": "The Tags Schema",
          "items": {
            "$id": "#/properties/syscheck/properties/tags/items",
            "type": "string",
            "title": "The Items Schema",
            "default": "",
            "examples": [
              "Sample"
            ],
            "pattern": "^(.*)$"
          }
        },
        "changed_attributes": {
          "$id": "#/properties/syscheck/properties/changed_attributes",
          "type": "array",
          "title": "The Changed_attributes Schema",
          "items": {
            "$id": "#/properties/syscheck/properties/changed_attributes/items",
            "type": "string",
            "title": "The Items Schema",
            "default": "",
            "examples": [
              "size",
              "permission",
              "uid",
              "user_name",
              "gid",
              "group_name",
              "mtime",
              "inode",
              "md5",
              "sha1",
              "sha256"
            ],
            "pattern": "^(.*)$"
          }
        },
        "event": {
          "$id": "#/properties/syscheck/properties/event",
          "type": "string",
          "title": "The Event Schema",
          "default": "",
          "examples": [
            "modified"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "decoder": {
      "$id": "#/properties/decoder",
      "type": "object",
      "title": "The Decoder Schema",
      "required": [
        "name"
      ],
      "properties": {
        "name": {
          "$id": "#/properties/decoder/properties/name",
          "type": "string",
          "title": "The Name Schema",
          "default": "",
          "examples": [
            "syscheck_integrity_changed"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "location": {
      "$id": "#/properties/location",
      "type": "string",
      "title": "The Location Schema",
      "default": "",
      "examples": [
        "syscheck"
      ],
      "pattern": "^(.*)$"
    }
  }
}
