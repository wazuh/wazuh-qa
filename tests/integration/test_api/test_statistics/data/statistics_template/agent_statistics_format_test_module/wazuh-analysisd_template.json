                                              "ciscat": {
                                                "type": "integer"
                                              },
                                              "command": {
                                                "type": "integer"
                                              },
                                              "docker": {
                                                "type": "integer"
                                              },
                                              "gcp": {
                                                "type": "integer"
                                              },
                                              "github": {
                                                "type": "integer"
                                              },
