                                                "type": "integer"
                                              },
                                              "sca": {
                                                "type": "integer"
                                              },
                                              "syscheck": {
                                                "type": "integer"
                                              },
                                              "syscollector": {
                                                "type": "integer"
                                              },
                                              "upgrade": {
                                                "type": "integer"
                                              },
                                              "vulnerability": {
                                                "type": "integer"
                                              }
                                            },
                                            "required": [
                                              "aws",
                                              "azure",
                                              "ciscat",
                                              "command",
                                              "docker",
                                              "gcp",
                                              "github",
